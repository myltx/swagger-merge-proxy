{
  "name": "swagger-merge-proxy",
  "version": "1.0.0",
  "description": "A Node.js service to merge Springfox Swagger v2 docs for Apifox import",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "serve": "PM2_HOME=\"$(pwd)/.pm2\" pm2 start ecosystem.config.cjs",
    "serve:stop": "PM2_HOME=\"$(pwd)/.pm2\" pm2 stop swagger-merge-proxy",
    "serve:restart": "PM2_HOME=\"$(pwd)/.pm2\" pm2 restart swagger-merge-proxy",
    "serve:logs": "PM2_HOME=\"$(pwd)/.pm2\" pm2 logs swagger-merge-proxy"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "lodash": "^4.17.21"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
